generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tokens {
  user_id Int
  token   String
  id      Int    @id @default(autoincrement())
}

model users {
  username   String
  email      String
  password   String
  id         Int     @id @default(autoincrement())
  isactivate Boolean @default(false)
}
